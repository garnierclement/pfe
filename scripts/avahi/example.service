<?xml version="1.0" standalone='no'?>
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
  <name replace-wildcards="yes">EXAMPLE</name>
  <service>
    <type>_EXAMPLE._tcp</type>
    <port>EXAMPLE_PORT</port>
    <txt-record>path=/data/shared/Music</txt-record>
  </service>
</service-group>